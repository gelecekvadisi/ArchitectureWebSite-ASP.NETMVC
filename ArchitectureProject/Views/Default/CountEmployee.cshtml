
@using ArchitectureProject.Models.Entity
@model List<Statistic>

<!-- start count -->
<section class="count-one section-padding-bottom-xl section-padding-top">
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-6 col-xl-3 amin-count-funcat">
                    <div class="count-one-border position-relative py-3 py-sm-3 py-lg-4 py-xl-0">
                        <div class="counter-container">
                            <h1 class="h1 dark-text"><span class="amin-auto-count">@item.Data</span>+</h1>
                            <h5 class="h5 fw-400 line-height-1 body-text">
                                @item.Title
                            </h5>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
<!-- end count -->