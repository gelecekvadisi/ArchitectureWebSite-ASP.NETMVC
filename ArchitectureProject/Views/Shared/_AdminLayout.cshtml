
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@Html.Action("HeadPartial", "AdminLayout")

<!-- body start -->
<body class="loading" data-layout-color="light" data-layout-mode="default" data-layout-size="fluid" data-topbar-color="light" data-leftbar-position="fixed" data-leftbar-color="light" data-leftbar-size='default' data-sidebar-user='true'>

    <!-- Begin page -->
    <div id="wrapper">


        <!-- Topbar Start -->
        @Html.Action("NavbarPartial", "AdminLayout")
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start ========== -->
        @Html.Action("LeftSideMenuPartial", "AdminLayout")
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <div class="content">
                <!-- Start Content-->
                <div class="container-fluid">

                    @RenderBody()

                </div> <!-- container-fluid -->
            </div> <!-- content -->
            <!-- Footer Start -->
            @Html.Action("FooterPartial", "AdminLayout")
            <!-- end Footer -->

        </div>
        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->
    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>

    <!-- Vendor -->
    @Html.Action("ScriptPartial", "AdminLayout")

</body>
</html>